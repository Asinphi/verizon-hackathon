{
    "manifest_version": 2,
    "name": "Chatbot Extension",
    "description": "This extension enables a chatbot",
    "version": "1.0",
    "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
    },
    "permissions": [
      "storage", "activeTab", "<all_urls>"
    ],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
  }